   196                                  ;Function information
   197                                  ;  Function name: is_digit
   198                                  ;  Name selection.  This function is named is_digit to avoid confusion with an existing library function named isdigit.
   199                                  ;  Programming language: X86 assembly in Intel syntax.
   200                                  ;  Date development began:  2022-Feb-28
   201                                  ;  Date version 1.0 finished: 2022-Feb 28
   202                                  ;  Files of this function: currently is_digit is an auxillary function of isfloat, and as such does not occupy its own file.
   203                                  ;  System requirements: an X86 platform with nasm installed or other compatible assembler.
   204                                  ;  Known issues: none
   205                                  ;  Assembler used for testing: Nasm version 2.14.02
   206                                  ;  Prototype: bool is_digit(char);
   207                                  ;
   208                                  ;Purpose
   209                                  ;  This function wil accept a single char as input parameter and determine if that parameter represents a decimal digit. 
   210                                  ;
   211                                  ;Translation information if this function occupied its own file.  Currently the function is_digit resides in the same 
   212                                  ;same file as isfloat and therefore, will be assembled when isfloat is assembled.
   213                                  ;  Assemble: nasm -f elf64 -l is_digit.lis -o is_digit.o is_digit.asm
   214                                  ;
   215                                  ;Software design document:
   216                                  ;  An Execution flow chart accompanies this function.  That document will provide a better understanding of the 
   217                                  ;  algorithm used in the isfloat function than a direct reading of the source code of the function.
   218                                  
   219                                  ;========= Begin source code ====================================================================================
   220                                  ;Declaration area
   221                                  true equ -1
   222                                  false equ 0
   223                                  ascii_value_of_zero equ 0x30
   224                                  ascii_value_of_nine equ 0x39
   225                                  
   226                                  segment .data
   227                                     ;This segment is empty
   228                                  
   229                                  segment .bss
   230                                     ;This segment is empty
   231                                  
   232                                  segment .text
   233                                  is_digit:
   234                                  
   235                                  ;Block that backs up almost all GPRs
   236                                  ;Back up the general purpose registers for the sole purpose of protecting the data of the caller.
   237 0000009F 55                      push rbp                                          ;Backup rbp
   238 000000A0 4889E5                  mov  rbp,rsp                                      ;The base pointer now points to top of stack
   239 000000A3 57                      push rdi                                          ;Backup rdi
   240 000000A4 56                      push rsi                                          ;Backup rsi
   241 000000A5 52                      push rdx                                          ;Backup rdx
   242 000000A6 51                      push rcx                                          ;Backup rcx
   243 000000A7 4150                    push r8                                           ;Backup r8
   244 000000A9 4151                    push r9                                           ;Backup r9
   245 000000AB 4152                    push r10                                          ;Backup r10
   246 000000AD 4153                    push r11                                          ;Backup r11
   247 000000AF 4154                    push r12                                          ;Backup r12
   248 000000B1 4155                    push r13                                          ;Backup r13
   249 000000B3 4156                    push r14                                          ;Backup r14
   250 000000B5 4157                    push r15                                          ;Backup r15
   251 000000B7 53                      push rbx                                          ;Backup rbx
   252 000000B8 9C                      pushf                                             ;Backup rflags
   253                                  
   254                                  ;Make a copy of the passed in array of ascii values.
   255                                  ;Note that only the low one-byte of rdi is important for this function is_digit.
   256                                  ;Nevertheless, we copy the entire 8-byte register.
   257 000000B9 41BD00000000            mov r13,0
   258 000000BF 4188FD                  mov r13b,dil     ;Copy the low byte of rdi to the low byte of r13.  The other bytes of rdi are all zeros.
   259                                  
   260                                  ;Block to test if value in r13 >= ascii(0)
   261 000000C2 4983FD30                cmp r13,ascii_value_of_zero
   262 000000C6 7C12                    jl is_digit.return_false
   263                                  
   264                                  ;Block to test if value in r13 <= ascii(9)
   265 000000C8 4983FD39                cmp r13,ascii_value_of_nine
   266 000000CC 7F0C                    jg is_digit.return_false
   267                                  
   268                                  ;Return true
   269 000000CE 4831C0                  xor rax,rax  ;Set rax to zero
   270 000000D1 48C7C0FFFFFFFF          mov rax,true
   271 000000D8 EB08                    jmp is_digit.restore_gpr_registers
   272                                  
   273                                  is_digit.return_false:
   274 000000DA 4831C0                  xor rax,rax  ;Set rax to zero
   275 000000DD B800000000              mov rax,false
   276                                  
   277                                  is_digit.restore_gpr_registers:
   278                                  ;Restore all general purpose registers to their original values
   279 000000E2 9D                      popf                                    ;Restore rflags
   280 000000E3 5B                      pop rbx                                 ;Restore rbx
   281 000000E4 415F                    pop r15                                 ;Restore r15
   282 000000E6 415E                    pop r14                                 ;Restore r14
   283 000000E8 415D                    pop r13                                 ;Restore r13
   284 000000EA 415C                    pop r12                                 ;Restore r12
   285 000000EC 415B                    pop r11                                 ;Restore r11
   286 000000EE 415A                    pop r10                                 ;Restore r10
   287 000000F0 4159                    pop r9                                  ;Restore r9
   288 000000F2 4158                    pop r8                                  ;Restore r8
   289 000000F4 59                      pop rcx                                 ;Restore rcx
   290 000000F5 5A                      pop rdx                                 ;Restore rdx
   291 000000F6 5E                      pop rsi                                 ;Restore rsi
   292 000000F7 5F                      pop rdi                                 ;Restore rdi
   293 000000F8 5D                      pop rbp                                 ;Restore rbp
   294                                  
   295 000000F9 C3                      ret                                     ;Pop the integer stack and jump to the address represented by the popped value.
