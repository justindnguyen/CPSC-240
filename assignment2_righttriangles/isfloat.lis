    26                                  ;Function information
    27                                  ;  Function name: isfloat
    28                                  ;  Programming language: X86 assembly in Intel syntax.
    29                                  ;  Date development began:  2022-Feb-28
    30                                  ;  Date version 1.0 finished: 2022-Mar-03
    31                                  ;  Files of this function: isfloat.asm
    32                                  ;  System requirements: an X86 platform with nasm installed o other compatible assembler.
    33                                  ;  Know issues: <now in testing phase>
    34                                  ;  Assembler used for testing: Nasm version 2.14.02
    35                                  ;  Prototype: bool isfloat(char *);
    36                                  ;
    37                                  ;Purpose
    38                                  ;  This function wil accept a string (array of char) and verify that it can be converted to a corresponding 64-bit 
    39                                  ;  float number or not converted to a float number.
    40                                  ;
    41                                  ;Translation information
    42                                  ;  Assemble: nasm -f elf64 -l isfloat.lis -o isfloat.o isfloat.asm
    43                                  ;
    44                                  ;Software design document:
    45                                  ;  An Execution flow chart accompanies this function.  That document will provide a better understanding of the 
    46                                  ;  algorithm used in the isfloat function than a direct reading of the source code of the function.
    47                                  ;
    48                                  ;
    49                                  ;========= Begin source code ====================================================================================
    50                                  ;Declaration area
    51                                  
    52                                  global isfloat
    53                                  
    54                                  null equ 0
    55                                  true equ -1
    56                                  false equ 0
    57                                  
    58                                  segment .data
    59                                     ;This segment is empty
    60                                  
    61                                  segment .bss
    62                                     ;This segment is empty
    63                                  
    64                                  segment .text
    65                                  isfloat:
    66                                  
    67                                  ;Block that backs up almost all GPRs
    68                                  ;Back up the general purpose registers for the sole purpose of protecting the data of the caller.
    69 00000000 55                      push rbp                                          ;Backup rbp
    70 00000001 4889E5                  mov  rbp,rsp                                      ;The base pointer now points to top of stack
    71 00000004 57                      push rdi                                          ;Backup rdi
    72 00000005 56                      push rsi                                          ;Backup rsi
    73 00000006 52                      push rdx                                          ;Backup rdx
    74 00000007 51                      push rcx                                          ;Backup rcx
    75 00000008 4150                    push r8                                           ;Backup r8
    76 0000000A 4151                    push r9                                           ;Backup r9
    77 0000000C 4152                    push r10                                          ;Backup r10
    78 0000000E 4153                    push r11                                          ;Backup r11
    79 00000010 4154                    push r12                                          ;Backup r12
    80 00000012 4155                    push r13                                          ;Backup r13
    81 00000014 4156                    push r14                                          ;Backup r14
    82 00000016 4157                    push r15                                          ;Backup r15
    83 00000018 53                      push rbx                                          ;Backup rbx
    84 00000019 9C                      pushf                                             ;Backup rflags
    85                                  
    86                                  
    87                                  ;Make a copy of the passed in array of ascii values
    88 0000001A 4989FD                  mov r13, rdi                                      ;r13 is the array of char
    89                                  
    90                                  ;Let r14 be an index of the array r13.  Initialize to integer 0
    91 0000001D 4D31F6                  xor r14, r14
    92                                  
    93                                  ;Check for leading plus or minus signs
    94 00000020 41807D002B              cmp byte [r13],'+'
    95 00000025 7407                    je increment_index
    96 00000027 41807D002D              cmp byte[r13],'-'
    97 0000002C 7503                    jne continue_validation
    98                                  increment_index:
    99 0000002E 49FFC6                  inc r14
   100                                  
   101                                  continue_validation:
   102                                  
   103                                  ;Block: loop to validate chars before the decimal point
   104                                  loop_before_point:
   105 00000031 B800000000                 mov rax,0
   106 00000036 4831FF                     xor rdi,rdi                ;Zero out rdi
   107 00000039 438A7C3500                 mov dil,byte [r13+1*r14]   ;dil is the low byte in the register rdi; reference Jorgensen, p. 10
   108 0000003E E85C000000                 call is_digit
   109 00000043 4883F800                   cmp rax,false
   110 00000047 7405                       je is_it_radix_point
   111 00000049 49FFC6                     inc r14
   112 0000004C EBE3                       jmp loop_before_point
   113                                  ;End of loop checking chars before the point is encountered.
   114                                  
   115                                  is_it_radix_point:
   116                                  
   117                                  ;Is the next value of the array a genuine radix point?
   118 0000004E 43807C35002E            cmp byte[r13+1*r14],'.'
   119 00000054 752C                        jne return_false
   120                                  
   121                                  ;A point has been found, therefore, begin a loop to process remaining digits.
   122                                  start_loop_after_finding_a_point:
   123 00000056 49FFC6                      inc r14
   124 00000059 B800000000                  mov rax,0
   125 0000005E 4831FF                      xor rdi,rdi
   126 00000061 438A7C3500                  mov dil,byte[r13+1*r14]
   127 00000066 E834000000                  call is_digit
   128 0000006B 4883F800                    cmp rax,false
   129 0000006F 75E5                        jne start_loop_after_finding_a_point
   130                                  ;End of loop processing valid digits after passing the one decimal point.
   131                                  
   132                                  ;Something other than a digit has been found.  
   133                                  ;It should be null at the end of the string.
   134 00000071 43807C350000            cmp byte [r13+1*r14],null
   135 00000077 7509                    jne return_false
   136 00000079 48C7C0FFFFFFFF          mov rax,true
   137 00000080 EB05                    jmp restore_gpr_registers
   138                                      
   139                                  return_false:
   140 00000082 B800000000              mov rax,false
   141                                  
   142                                  restore_gpr_registers:
   143 00000087 9D                      popf                                    ;Restore rflags
   144 00000088 5B                      pop rbx                                 ;Restore rbx
   145 00000089 415F                    pop r15                                 ;Restore r15
   146 0000008B 415E                    pop r14                                 ;Restore r14
   147 0000008D 415D                    pop r13                                 ;Restore r13
   148 0000008F 415C                    pop r12                                 ;Restore r12
   149 00000091 415B                    pop r11                                 ;Restore r11
   150 00000093 415A                    pop r10                                 ;Restore r10
   151 00000095 4159                    pop r9                                  ;Restore r9
   152 00000097 4158                    pop r8                                  ;Restore r8
   153 00000099 59                      pop rcx                                 ;Restore rcx
   154 0000009A 5A                      pop rdx                                 ;Restore rdx
   155 0000009B 5E                      pop rsi                                 ;Restore rsi
   156 0000009C 5F                      pop rdi                                 ;Restore rdi
   157 0000009D 5D                      pop rbp                                 ;Restore rbp
   158                                  
   159 0000009E C3                      ret                                     ;Pop the integer stack and jump to the address represented by the popped value.
